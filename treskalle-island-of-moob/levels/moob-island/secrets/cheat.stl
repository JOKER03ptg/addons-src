(supertux-level
  (version 2)
  (name (_ "How to Cheat"))
  (author "treskalle")
  (license "GPL 2+ / CC-by-sa 3.0")
  (sector
    (name "main")
    (ambient-light 1 1 1)
    (camera
      (mode "normal")
    )
    (infoblock
      (message (_ "#How many times haven't you wondered about how you can skip a level in SuperTux?"))
      (x 544)
      (y 96)
    )
    (infoblock
      (message (_ "#Yes, it's possible. And even easy."))
      (x 832)
      (y 96)
    )
    (infoblock
      (message (_ "-Step 1:
#Locate your (hidden) (.)supertux2 directory using your file browser.
#On Linux it's in most cases /home/username/.supertux2 (~/.supertux2), Ctrl+H shows hidden files."))
      (x 1184)
      (y 352)
    )
    (infoblock
      (message (_ "-Step 3-1:
#If you want to skip a level in this worldmap, open the .stsg-file 'moob-island' with your favorite text editor (Text editors isn't applications like OpenOffice.org Writer or Microsoft Word (I hate MS Office), try Gedit, Notepad(++) or something else)."))
      (x 672)
      (y 352)
    )
    (infoblock
      (message (_ "-Step 2:
#From this directory, locate your profile folder. If your profile has the number '4', its directory is 'profile4'.
#If you don't know what a profile is, or haven't changed your main profile, the directory's name is 'profile1'."))
      (x 896)
      (y 352)
    )
    (infoblock
      (message (_ "-Step 4:
#In the file you opened, you'll probably see something like this:
#(\"levels/tower.stl\"
#(solved #f)
#)"))
      (x 288)
      (y 640)
    )
    (infoblock
      (message (_ "-Step 3-2:
#Other worldmaps have names like 'world1', 'world2', 'mattie_world', etc. Please try it yourself."))
      (x 416)
      (y 352)
    )
    (infoblock
      (message (_ "-Step 6-1:
#(solved #f) means that you haven't finished this level.
#Change it to (solved #t), that means that you have finished this level."))
      (x 544)
      (y 640)
    )
    (infoblock
      (message (_ "-Step 6-2:
#If you don't know the file name of the level you want to skip, try changing all of the #f-s to #t."))
      (x 608)
      (y 640)
    )
    (infoblock
      (message (_ "-Warning!
#SuperTux should not run while you edit the file."))
      (x 832)
      (y 640)
    )
    (spawnpoint
      (name "main")
      (x 224)
      (y 96)
    )
